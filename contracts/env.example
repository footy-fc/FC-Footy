# Contract Deployment Environment Variables
# Copy this file to .env and fill in your values

# Private key for contract deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Base Network RPC URLs
BASE_RPC_URL=https://mainnet.base.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# BaseScan API Key for contract verification
BASESCAN_API_KEY=your_basescan_api_key_here

# Optional: Custom RPC URLs (if using Alchemy, Infura, etc.)
# BASE_RPC_URL=https://base-mainnet.g.alchemy.com/v2/YOUR_API_KEY
# BASE_SEPOLIA_RPC_URL=https://base-sepolia.g.alchemy.com/v2/YOUR_API_KEY

# Community wallet address for ScoreSquare contract
COMMUNITY_WALLET=0x0000000000000000000000000000000000000000
